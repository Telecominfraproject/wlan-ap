. /lib/functions/leds.sh
. /lib/functions/uci-defaults.sh

board=$(board_name)

board_config_update

case $board in
edgecore,eap111)
	ucidef_set_led_netdev "wan" "wan" "orange:wan" "eth0"
	ucidef_set_led_netdev "wlan5g" "WLAN5G" "blue:wlan5g" "wlan1"
	;;
edgecore,eap112)
	ucidef_set_led_netdev "wan" "wan" "orange:wan" "eth0"
	ucidef_set_led_netdev "wlan5g" "WLAN5G" "blue:wlan5g" "wlan2"
	;;
esac

board_config_flush

exit 0
