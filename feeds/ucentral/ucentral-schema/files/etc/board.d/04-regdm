#!/bin/sh

. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
cig,wf186w|\
cig,wf186h|\
cig,wf194c4|\
cig,wf194c|\
cig,wf610d|\
cig,wf808|\
cig,wf660a)
        ucidef_set_wifi_country 'US'
        ;;
cig,wf188n)
        ucidef_set_wifi_country "US CA MY DE"
	;;
cig,wf196)
        ucidef_set_wifi_country "US CA"
        ;;
sonicfi,rap630c-311g|\
sonicfi,rap630w-311g)
        ucidef_set_wifi_country "US CA"
        ;;
esac

board_config_flush

exit 0
