+&qpic_bam {
+	status = "okay";
+};
+
+&wifi0 {
+	status = "okay";
+	qcom,ath10k-calibration-variant = "tp-link AP2220";
+};
+
+&wifi1 {
+	status = "disabled";
+	qcom,ath10k-calibration-variant = "tp-link AP2220";
+};
+
+&pcie0 {
+	status = "okay";
+	perst-gpio = <&tlmm 38 GPIO_ACTIVE_LOW>;
+	wake-gpio = <&tlmm 40 GPIO_ACTIVE_LOW>;
+
+	bridge@0,0 {
+    	reg = <0x00000000 0 0 0 0>;
+		#address-cells = <3>;
+        #size-cells = <2>;
+        ranges;
+
+    	wifi2: wifi@1,0 {
+            compatible = "qcom,ath10k";
+            status = "okay";
+            reg = <0x00010000 0 0 0 0>;
+            qcom,ath10k-calibration-variant = "tp-link AP2220";
+        };
+	};
+};
